<?xml version="1.0" encoding="UTF-8" ?>
        <!ELEMENT calendar (users*,permAppointments*,user*, currentUser*)>
        <!ELEMENT currentUser (#PCDATA)>
        <!ATTLIST currentUser
                id CDATA #REQUIRED>
        <!ELEMENT users (User*)>
        <!ELEMENT User (permAppointment*)>
        <!ATTLIST User
                id CDATA #REQUIRED>
        <!ELEMENT permAppointments (User*)>
        <!ELEMENT permAppointment (#PCDATA)>
        <!ATTLIST permAppointment
                start CDATA #REQUIRED
                end CDATA #REQUIRED
                repetition CDATA #REQUIRED
                weekday CDATA #REQUIRED>
        <!ELEMENT user (years*)>
        <!ATTLIST user
                id CDATA #REQUIRED>
        <!ELEMENT years (month*)>
        <!ATTLIST years
                id CDATA #REQUIRED>
        <!ELEMENT month (day*)>
        <!ATTLIST month
                id CDATA #REQUIRED>
        <!ELEMENT day (period*)>
        <!ATTLIST day
                date CDATA #REQUIRED
                blocked CDATA #REQUIRED>
        <!ELEMENT period (#PCDATA)>
        <!ATTLIST period
                start CDATA #REQUIRED
                end CDATA #REQUIRED
                isfamEvent CDATA #REQUIRED>
